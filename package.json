{
  "name": "task-manager",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "json-server --watch db.json --port $PORT",
    "server:local": "json-server --watch db.json --port 3001"
  },
  "dependencies": {
    "@heroui/react": "^2.7.11",
    "axios": "^1.10.0",
    "framer-motion": "^11.9.0",
    "json-server": "^1.0.0-beta.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-svg": "^16.3.0",
    "sass": "^1.89.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
